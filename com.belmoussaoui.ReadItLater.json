{
  "id": "com.belmoussaoui.ReadItLater",
  "runtime": "org.gnome.Platform",
  "runtime-version": "47",
  "sdk": "org.gnome.Sdk",
  "sdk-extensions": [
    "org.freedesktop.Sdk.Extension.rust-stable"
  ],
  "command": "read-it-later",
  "finish-args": [
    "--share=ipc",
    "--socket=fallback-x11",
    "--socket=wayland",
    "--socket=pulseaudio",
    "--device=dri",
    "--share=network",
    "--talk-name=org.freedesktop.secrets"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin",
    "env": {
      "RUSTFLAGS": "--remap-path-prefix =../",
      "RUST_LOG": "read_it_later=info"
    }
  },
  "modules": [
    {
      "name": "read-it-later",
      "buildsystem": "meson",
      "sources": [
        {
          "type": "archive",
          "url": "https://gitlab.gnome.org/-/project/9133/uploads/63c635eb6c0270ec6ae9838e84a7e76b/read-it-later-0.6.1.tar.xz",
          "sha256": "aa455601e6d73eb0dd886df03a2cc4ef28439ca1f9b16b66c7a29a37d60319cd"
        }
      ]
    }
  ]
}
