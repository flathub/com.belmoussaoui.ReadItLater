{
  "id": "com.belmoussaoui.ReadItLater",
  "runtime": "org.gnome.Platform",
  "runtime-version": "43",
  "sdk": "org.gnome.Sdk",
  "sdk-extensions": ["org.freedesktop.Sdk.Extension.rust-stable"],
  "command": "read-it-later",
  "finish-args": [
    "--share=ipc",
    "--socket=fallback-x11",
    "--socket=wayland",
    "--socket=pulseaudio",
    "--device=dri",
    "--share=network",
    "--talk-name=org.freedesktop.secrets"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin",
    "env": {
      "RUSTFLAGS": "--remap-path-prefix =../",
      "RUST_LOG": "read_it_later=info"
    }
  },
  "modules": [
    {
      "name": "read-it-later",
      "buildsystem": "meson",
      "sources": [
        {
          "type": "archive",
          "url": "https://gitlab.gnome.org/World/read-it-later/uploads/6ec4ca799368abb58ca080864bc24e9c/read-it-later-0.3.0.tar.xz",
          "sha256": "ac1c50e7894e0e193e8b2aba4b05bc807f69f66470277e6023aafcf3227c468d"
        }
      ]
    }
  ]
}
